<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style type="com.calclab.emite.xfunctional.client.ui.OutputMessage.Styles">
		.info {
			color: #00c;
		}
		
		.debug {
			color: gray;
		}
		
		.fail {
			color: #c00;
		}
		
		.success {
			color: #0c0;
		}
	</ui:style>
	<g:FlowPanel>
		<g:Label ui:field="body"></g:Label>
	</g:FlowPanel>
</ui:UiBinder> 